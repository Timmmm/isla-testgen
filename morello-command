# pac and tagged memory instructions not supported on morello
# exclusive instructions not supported without memory setup
# eret needs more registers set
target/release/isla-testgen -a morello -T 1 -C morello.toml -A ../arm-morello-dropzone/ir/morello.ir -t ../arm-morello-dropzone/2020-09-02_asl_morello/v8a64.tag -t ../arm-morello-dropzone/2020-09-02_asl_morello/siris.tag --max-retries 20 -k Halt -k AArch64_TakeException -k AArch64_DataAbort --exclude /pac/  --exclude /vector/ --exclude /float/ --exclude /pointer/mc --exclude /tags/mc --exclude memory/ordered-rcpc --exclude lda_stl --exclude memory/exclusive --exclude integer/crc --exclude memory/atomicops --exclude '^bl?ra:' --exclude '^e?reta:' --exclude '^eret:' --exclude '^wf[ei]:' --exclude '^m(sr|rs)' --exclude '^sys' --exclude '^LDAP?RB?' --exclude '^LDA?X[PR]' --exclude '^STLRB?' --exclude '^STL?X[PR]' --exclude '^SWPAL?' --exclude '^SWPL' -L AddWithCarry -L CapGetExponent -L CapGetBottom -L CapGetTop -L CapIsExponentOutOfRange -L CapIsRangeInBounds -L fdiv_int -L CapGetBounds -L CapSetBounds -L Bit _
